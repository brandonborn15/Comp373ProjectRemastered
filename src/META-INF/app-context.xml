<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

<discription>Wiring the dependencies for the facility management application.</description>
<context:componet-scan base-package="Comp373ProjectRemastered" />

<bean id="facilityUse" class="facility_use.FacilityUseImpl" scope="prototype"/>

<bean id="maintenance" class="facility_maintenance.FacilityMaintenanceImpl" scope="prototype"/>

<bean id="facility" class="facility.FacilityImpl" scope="prototype"/>

<bean id="generalUse" class="facility_use.GeneralUseImpl" scope="prototype">

<bean id="inspection" class="facility_use.InspectionImpl" scope="prototype">

<bean id="management" class="facility_management.FacilityManagementImpl" scope="prototype">

<bean id="request" class="facility_maintenance.RequestImpl" scope="prototype">

<bean id="sechdule" class="facility_maintenance.ScheduleMaintenanceImpl" scope="prototype">

</beans>


